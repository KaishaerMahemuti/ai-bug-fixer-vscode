{
  "name": "ai-bug-fixer",
  "displayName": "AI Bug Fixer",
  "description": "An AI-powered bug fixer for VS Code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Other"
  ],
  "main": "src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ai-bug-fixer.analyzeError",
        "title": "Analyze Error with AI"
      }
    ],
    "configuration": {
      "properties": {
        "aiBugFixer.openaiApiKey": {
          "type": "string",
          "description": "Your OpenAI API Key for AI Bug Fixer",
          "scope": "machine"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.97.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.19.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
